<?xml version="1.0" ?>
<config xsi:schemaLocation="http://joo.org/steak steak-0.1.0.xsd">
	<states>
		<state>
			<name>default</name>
			<value>org.joo.steak.test.states.DefaultTestState</value>
		</state>
		<state>
			<name>add</name>
			<value>org.joo.steak.test.states.AddTestState</value>
		</state>
		<state>
			<name>multiply</name>
			<value>org.joo.steak.test.states.MultiplyTestState</value>
		</state>
		<state>
			<name>subtract</name>
			<value>org.joo.steak.test.states.SubtractTestState</value>
		</state>
		<state>
			<name>divide</name>
			<value>org.joo.steak.test.states.DivideTestState</value>
		</state>
	</states>
	<flows>
		<flow>
			<from>default</from>
			<action>123</action>
			<action>*</action>
			<transitions>
				<transition>add</transition>
			</transitions>
		</flow>
		<flow>
			<from>default</from>
			<action>*</action>
			<transitions>
				<transition>add</transition>
			</transitions>
		</flow>
		<flow>
			<from>add</from>
			<action>*</action>
			<transitions>
				<transition>multiply</transition>
			</transitions>
		</flow>
		<flow>
			<from>multiply</from>
			<action>*</action>
			<transitions>
				<transition>subtract</transition>
			</transitions>
		</flow>
		<flow>
			<from>subtract</from>
			<action>*</action>
			<transitions>
				<transition>divide</transition>
			</transitions>
		</flow>
	</flows>
</config>